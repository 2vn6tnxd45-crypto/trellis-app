{
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    },
    {
      "source": "/home",
      "destination": "/home/index.html"
    },
    {
      "source": "/home/(.*)",
      "destination": "/home/index.html"
    }
  ],
  "crons": [
    {
      "path": "/api/cron/weekly-digest",
      "schedule": "0 9 * * 1"
    },
    {
      "path": "/api/cron/appointment-reminders",
      "schedule": "0 7,18 * * *"
    },
    {
      "path": "/api/cron/completion-reminders",
      "schedule": "0 9 * * *"
    },
    {
      "path": "/api/cron/rating-reminders",
      "schedule": "0 10 * * *"
    },
    {
      "path": "/api/cron/review-requests",
      "schedule": "0 * * * *"
    },
    {
      "path": "/api/cron/sms-reminders",
      "schedule": "*/15 * * * *"
    },
    {
      "path": "/api/cron/membership-renewals",
      "schedule": "0 8 * * *"
    }
  ]
}
